    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "src/streamlit_app/main.py"
            ]
        },
        {
            "name": "import_netflix",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "import_netflix"
